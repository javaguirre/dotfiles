// Place your settings in this file to overwrite the default settings
{
"editor.fontFamily": "Dank Mono, JetBrains Mono, Input Mono Narrow, Monaco, 'Courier New', monospace",
"editor.fontLigatures": true,
"terminal.integrated.fontFamily": "\"SourceCodePro+Powerline+Awesome Regular\", Dank Mono, Operator Mono, Mononoki, Operator Mono, Menlo, Monaco, 'Courier New', monospace",
"terminal.explorerKind": "external",
"terminal.external.osxExec": "iTerm.app",
"vim.startInInsertMode": false,
"docker.dockerComposeBuild": false,
"docker.dockerComposeDetached": false,
"vim.useSystemClipboard": true,
"vim.insertModeKeyBindings": [
    {
        "before": ["j","k"],
        "after": ["<Esc>"]
    }
],
"vim.normalModeKeyBindings": [
    {
        "before": ["leader", "a"],
        "after": [],
        "commands": [
            {
                "command": "workbench.view.search",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", ">"],
        "after": [],
        "commands": [
            {
                "command": "workbench.action.terminal.focus",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "b"],
        "after": [],
        "commands": [
            {
                "command": "workbench.action.quickOpen",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "c"],
        "after": [],
        "commands": [
            {
                "command": "editor.action.commentLine",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "d"],
        "after": [],
        "commands": [
            {
                "command": "editor.action.formatDocument",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "e"],
        "after": [],
        "commands": [
            {
                "command": "workbench.actions.view.problems",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "f"],
        "after": [],
        "commands": [
            {
                "command": "workbench.action.quickOpen",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "p"],
        "after": [],
        "commands": [
            {
                "command": "workbench.action.showCommands",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "l"],
        "after": [],
        "commands": [
            {
                "command": "workbench.action.gotoLine",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "n"],
        "after": [],
        "commands": [
            {
                "command": "editor.action.marker.next",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "r"],
        "after": [],
        "commands": [
            {
                "command": "editor.action.rename",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "t"],
        "after": [],
        "commands": [
            {
                "command": "workbench.action.showAllSymbols",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "x"],
        "after": [],
        "commands": [
            {
                "command": "workbench.action.gotoSymbol",
                "args": []
            }
        ]
    },
    {
        "before": ["leader", "leader"],
        "after": [],
        "commands": [
            {
                "command": "editor.action.commentLine",
                "args": []
            }
        ]
    }
],
"vim.visualModeKeyBindings": [
    {
        "before": ["leader", "leader"],
        "after": [],
        "commands": [
            {
                "command": "editor.action.commentLine",
                "args": []
            }
        ]
    }
],
"files.trimTrailingWhitespace": true,
"files.insertFinalNewline": true,
"editor.minimap.enabled": false,
"editor.rulers": [80],
"explorer.confirmDelete": false,
"gitlens.keymap": "chorded",
"gitlens.codeLens.enabled": false,
"gitlens.currentLine.enabled": false,
"gitlens.hovers.currentLine.over": "line",
"gitlens.hovers.enabled": false,
"editor.bracketPairColorization.enabled": true,
"explorer.confirmDragAndDrop": false,
"gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
},
"java.home": "/Library/Java/Home",
"workbench.colorCustomizations": {
    "terminal.background": "#222222",
    "terminal.foreground": "#f5f1f1",
    "[Paper Dark]": {
        "panel.background": "#555555",
        "sideBar.background": "#444444",
        "editor.background": "#222222"
    },
    "[Paper White]": {
        "editor.lineHighlightBackground": "#fff",
        "editor.lineHighlightBorder": "#aeccaa77"
    },
    "[Material Theme Darker High Contrast]": {
        "statusBar.background": "#666666",
        "panel.background": "#AAAAAA",
        "sideBar.background": "#444444"
    },
},
"git.autofetch": true,
"java.errors.incompleteClasspath.severity": "ignore",
"files.exclude": {
    "**/.git": true,
    "**/.pyc": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
},
"javascript.updateImportsOnFileMove.enabled": "never",
"editor.tabCompletion": "on",
"editor.suggest.localityBonus": true,
"git.rebaseWhenSync": true,
"git.confirmSync": false,
"vim.easymotionMarkerFontFamily": "Dank Mono, Consolas",
"gitlens.views.lineHistory.enabled": true,
"gitlens.views.repositories.avatars": false,
"trailing-spaces.trimOnSave": true,
"editor.parameterHints.enabled": false,
"search.exclude": {
    "**/__pycache__": true,
    "**/.venv": true
},
"workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
"[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"editor.suggestSelection": "first",
"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
"[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"prettier.requireConfig": false,
"editor.cursorSmoothCaretAnimation": true,
"[python]": {},
"terminal.integrated.fontWeightBold": "normal",
"vim.easymotion": true,
"workbench.colorTheme": "Boxy Monokai",
"editor.codeActionsOnSave": {

    "source.fixAll.eslint": true
},
"[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"eslint.format.enable": true,
"eslint.debug": true,
"eslint.alwaysShowStatus": true,
"liveshare.featureSet": "stable",
"python.languageServer": "Default",
"terminal.integrated.lineHeight": 1.1,
"workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
},
"notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
},
"[handlebars]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"redhat.telemetry.enabled": false,
"githubPullRequests.terminalLinksHandler": "github",
"security.workspace.trust.untrustedFiles": "open",
"editor.fontSize": 13,
"editor.tokenColorCustomizations": {
    "[Tokyo Night, Tokyo Night Storm, Tokyo Night Light]": { // or "[Tokyo Night Storm]"
        "textMateRules": [{
            "scope": [
                "comment",
                "meta.var.expr storage.type",
                "keyword.control.flow",
                "keyword.control.return",
                "meta.directive.vue punctuation.separator.key-value.html",
                "meta.directive.vue entity.other.attribute-name.html",
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js",
                "storage.modifier"
            ],
            "settings": {
                "fontStyle": ""
            }
        }]
    }
},
"window.zoomLevel": 1
}

